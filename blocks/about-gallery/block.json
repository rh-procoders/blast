{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "acf/about-gallery",
    "title": "About Gallery",
    "description": "A creative gallery layout with scattered images, value tags, and call-to-action for showcasing company culture",
    "category": "theme",
    "icon": "format-gallery",
    "keywords": ["gallery", "about", "team", "culture", "values", "photos"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "template.php",
        "blockVersion": 3
    },
    "supports": {
        "align": false,
        "anchor": true,
        "customClassName": true,
        "spacing": {
            "margin": ["top", "bottom", "left", "right"],
            "padding": ["top", "bottom", "left", "right"]
        },
        "color": {
            "background": true,
            "gradients": true,
            "text": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true
        }
    },
    "example": {
        "attributes": {
            "mode": "preview",
            "data": {
                "preview_image_help": "preview.png"
            }
        }
    },
    "usesContext": ["postId", "postType"],
    "textdomain": "blast-2025",
    "editorStyle": "file:./css/editor-style.css",
    "style": "file:./css/view-style.css"
}